<Window x:Class="ProcessInspection.Windows.LinkToPartType"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Part Type" Height="300" Width="388">
    <Grid Name="MainGrid">
        <DataGrid  Name="PartTypes" Margin="10,41,10,40" AutoGenerateColumns="True" EnableRowVirtualization="True" IsReadOnly="True" MouseDoubleClick="PartTypes_MouseDoubleClick"/>
        <Label Content="Select a Part Type:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Content="Cancel" IsCancel="True" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Name="ButtonCancel" Click="ButtonCancel_Click"/>
        <Button Content="OK" IsDefault="True" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Name="ButtonOK" Click="ButtonOK_Click"/>

    </Grid>
</Window>
